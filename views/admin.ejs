                                </div>
                            </div>
                        
                            <div class="card">
                                <div class="card-header" id="headingTwo">
                                    <h2 class="mb-0">
                                        <button class="btn btn-link btn-block text-left collapsed" type="button" data-toggle="collapse"
                                            data-target="#hotels-report" aria-expanded="true" aria-controls="hotels-report">
                                            Hotels reports
                                        </button>
                                    </h2>
                                </div>
                                <div id="hotels-report" class="collapse">
                                    <div class="card-body">
                                        <%- include('admin/hotels/report2.ejs', { data: hreport2 }) %>

                                        <%- include('admin/hotels/report1.ejs', { data: hreport1 }) %>
                                    </div>
                                </div>
                            </div>
                        
