                            <div class="card">
                                <div class="card-header" id="headingTwo">
                                    <h2 class="mb-0">
                                        <button class="btn btn-link btn-block text-left collapsed" type="button" data-toggle="collapse"
                                            data-target="#users-report" aria-expanded="true" aria-controls="users-report">
                                            Available Users
                                        </button>
                                    </h2>
                                </div>
                                <div id="users-report" class="collapse">
                                    <div class="card-body">
                                        <%- include('admin/users/report1.ejs', { data: report5 }) %>
                                    </div>
                                </div>
                            </div>

                            <div class="card">
                                <div class="card-header" id="headingOne">
                                    <h2 class="mb-0">
                                        <button class="btn btn-link btn-block text-left" type="button" data-toggle="collapse"
                                            data-target="#flights-report" aria-expanded="true" aria-controls="flights-report">
